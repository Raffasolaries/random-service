{
 "name": "random-service",
 "version": "1.0.0",
 "description": "simple services for random numbers",
 "main": "index.js",
 "scripts": {
  "test": "jest",
  "test:watch": "npx jest --watch",
  "build": "rimraf ./build && tsc",
  "start:dev": "nodemon",
  "start": "npm run build && node build/src/index.js"
 },
 "repository": {
  "type": "git",
  "url": "git+https://github.com/Raffasolaries/random-service.git"
 },
 "keywords": [
  "random",
  "numbers"
 ],
 "author": "Raffaele Sollecito",
 "license": "ISC",
 "bugs": {
  "url": "https://github.com/Raffasolaries/random-service/issues"
 },
 "homepage": "https://github.com/Raffasolaries/random-service#readme",
 "dependencies": {
  "jsonschema": "^1.4.0"
 },
 "devDependencies": {
  "@types/jest": "^26.0.24",
  "@types/jest-json-schema": "^2.1.3",
  "jest": "^27.0.6",
  "jest-json-schema": "^5.0.0",
  "nodemon": "^2.0.12",
  "rimraf": "^3.0.2",
  "ts-jest": "^27.0.4",
  "ts-node": "^10.1.0",
  "typescript": "^4.3.5"
 },
 "jest": {
  "globalSetup": "./build/tests/jest.config.js",
  "moduleFileExtensions": [
   "ts",
   "js",
   "json"
  ],
  "testMatch": [
   "**/build/**/*.test.js"
  ],
  "testEnvironment": "node"
 }
}
